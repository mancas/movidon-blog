{% set crumbs = getCrumbs() %}
{% if crumbs | length > 0 %}
<ol class="breadcrumb">
    <li>
        <a title="{{ 'Go back home' | trans }}" href="{{ path('admin_index') }}">
            <i class="fa fa-home"></i>
            {{ 'Home' | trans }}
        </a>
    </li>
    {% for key, value in crumbs %}
        <li {% if loop.last %}class="active"{% endif %}>
        {% if loop.last %}
            {{ key }}
        {% else %}
            <a title="{{ key }}" href="{{ value }}">
                {{ key }}
            </a>
        {% endif %}
        </li>
    {% endfor %}
</ol>
{% endif %}